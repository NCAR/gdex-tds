<?xml version="1.0" encoding="UTF-8"?>
<catalog name="VAPOR Sample Data" xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0" xmlns:xlink="http://www.w3.org/1999/xlink">
  <!--Top level dataset: Needed to set metadata for Files & Aggregations-->
  <dataset name="VAPOR Sample Data">
    <metadata inherited="true">
      <serviceName>all</serviceName>
      <dataFormat>NetCDF</dataFormat>
      <dataType>GRID</dataType>
      <documentation type="Rights">Freely Available</documentation>
      <documentation xlink:href="http://gdex.ucar.edu/datasets/d897007/" xlink:title="NCAR GDEX - VAPOR Sample Data(d897007)" />
      <documentation type="summary">A collection of various sample data for the VAPOR (Visualization and Analysis Platform for Ocean, Atmosphere, and Solar Researchers) software.</documentation>
      <creator>
        <name vocabulary="DIF">UCAR/NCAR/CISL/VETS</name>
        <contact url="none" />
      </creator>
      <authority>edu.ucar.gdex</authority>
      <publisher>
        <name vocabulary="DIF">NCAR/GDEX</name>
        <contact url="http://gdex.ucar.edu/" email="datahelp@ucar.edu" />
    </publisher>
    </metadata>
    <!--Files-->
    <datasetScan name="d897007 Files" path="files/d897007" location="/data/rda/data/d897007/">
      <metadata inherited="true">
        <serviceName>all</serviceName>
      </metadata>
      <filter>
        <exclude wildcard="*.html" />
        <exclude wildcard="*.x" />
        <exclude wildcard=".*" />
        <exclude wildcard=".*/" />
      </filter>
      <addDatasetSize />
