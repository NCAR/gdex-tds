<?xml version="1.0" encoding="UTF-8"?>
<catalog name="Climate Analysis Center 10-Year Climate Diagnostics Data Base" xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0" xmlns:xlink="http://www.w3.org/1999/xlink">
  <!--Top level dataset: Needed to set metadata for Files & Aggregations-->
  <dataset name="Climate Analysis Center 10-Year Climate Diagnostics Data Base">
    <metadata inherited="true">
      <serviceName>all</serviceName>
      <dataFormat>NetCDF</dataFormat>
      <dataType>GRID</dataType>
      <documentation type="Rights">Freely Available</documentation>
      <documentation xlink:href="http://gdex.ucar.edu/datasets/d218001/" xlink:title="NCAR GDEX - Climate Analysis Center 10-Year Climate Diagnostics Data Base(d218001)" />
      <documentation type="summary">This dataset contains long-term means of geopotential height,
    temperature, and wind for the troposphere from 1000mb up to
    100mb.  Means are computed for the period from 1979 to 1988 and
    are stored on a global latitude/longitude grid with a resolution
    of 2.5 degrees.</documentation>
      <creator>
        <name vocabulary="DIF">DOC/NOAA/NWS/NCEP/CPC</name>
        <contact url="none" />
      </creator>
      <authority>edu.ucar.gdex</authority>
      <publisher>
        <name vocabulary="DIF">NCAR/GDEX</name>
        <contact url="http://gdex.ucar.edu/" email="datahelp@ucar.edu" />
    </publisher>
    </metadata>
    <!--Files-->
    <datasetScan name="d218001 Files" path="files/d218001" location="/data/rda/data/d218001/">
      <metadata inherited="true">
        <serviceName>all</serviceName>
      </metadata>
      <filter>
        <exclude wildcard="*.html" />
        <exclude wildcard="*.x" />
        <exclude wildcard=".*" />
        <exclude wildcard=".*/" />
      </filter>
      <addDatasetSize />
    </datasetScan>
  </dataset>
</catalog>
