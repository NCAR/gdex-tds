<?xml version="1.0" encoding="UTF-8"?>
<catalog name="Input Data Repository" xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0" xmlns:xlink="http://www.w3.org/1999/xlink">
  <!--Top level dataset: Needed to set metadata for Files & Aggregations-->
  <dataset name="Input Data Repository">
    <metadata inherited="true">
      <serviceName>all</serviceName>
      <dataFormat>NetCDF</dataFormat>
      <dataType>GRID</dataType>
      <documentation type="Rights">Freely Available</documentation>
      <documentation xlink:href="http://gdex.ucar.edu/datasets/d651077/" xlink:title="NCAR GDEX - Input Data Repository(d651077)" />
      <documentation type="summary">This is the collection of input data required for CESM and its component models. </documentation>
      <creator>
        <name vocabulary="DIF">UCAR/NCAR/CGD</name>
        <contact url="none" />
      </creator>
      <authority>edu.ucar.gdex</authority>
      <publisher>
        <name vocabulary="DIF">NCAR/GDEX</name>
        <contact url="http://gdex.ucar.edu/" email="datahelp@ucar.edu" />
    </publisher>
    </metadata>
    <!--Files-->
    <datasetScan name="d651077 Files" path="files/d651077" location="/data/rda/data/d651077/">
      <metadata inherited="true">
        <serviceName>all</serviceName>
      </metadata>
      <filter>
        <exclude wildcard="*.html" />
        <exclude wildcard="*.x" />
        <exclude wildcard=".*" />
        <exclude wildcard=".*/" />
      </filter>
      <addDatasetSize />
    </datasetScan>
  </dataset>
</catalog>
