        xmlns:xlink="http://www.w3.org/1999/xlink">


  <dataset name="Analysis">

      <featureCollection name="Surface"
                         featureType="GRIB"
                         harvest="true"
                         path="aggregations/d@1/9">
    
        <metadata inherited="true">
             <serviceName>all</serviceName>
             <dataFormat>GRIB-1</dataFormat>
               <documentation type="summary">
               [EXPERIMENTAL INTERFACE :: Please see http://rda.ucar.edu/datasets/d@1 for original RDA Dataset Page]
              </documentation>
        </metadata>
    
        <collection name="d@1_an_sfc" 
                    spec="/data/rda/data/d@1/yt.oper.an.sfc/**/yt\.oper\.an\.sfc\..*"
                    olderThan="0 min"/>
    
           <gribConfig datasetTypes="Best">
             <bestNamer name="Timeseries Aggregation"/>
             <pdsHash>
               <useTableVersion>false</useTableVersion>
             </pdsHash>
           </gribConfig>
           <update startup="false" trigger="allow"/>
      </featureCollection>
    
  </dataset>

